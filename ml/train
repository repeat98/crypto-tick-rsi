python3 train.py \
  labels \
  gaf \
  --batch_size 64 \
  --epochs 20 \
  --lr 1e-4 \
  --wd 1e-5 \
  --workers 8 \
  --model_out best_model.pth


  python3 train_multiframe.py \
  labels_multiframe \
  gaf_multiframe \
  --batch_size 32 \
  --epochs 20 \
  --lr 1e-4 \
  --wd 1e-5 \
  --workers 2 \
  --model_out best_multiframe.pth